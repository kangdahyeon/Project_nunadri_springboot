	<select id="getMyHomeBoardList" resultType="HashMap">
		SELECT m.*, h.house_no, u.nickname
  		FROM notice_myhouse m
  		LEFT OUTER JOIN house h ON m.house_no = h.house_no
  		LEFT OUTER JOIN member u ON m.nickname = u.nickname
  		WHERE myhouse_category = 'h'
  		ORDER BY myhouse_no DESC
  	</select>
	
